<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css"> 
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css"> 
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.11.0/themes/smoothness/jquery-ui.css"> 
    <link href="~/Content/a.css" rel="stylesheet" />

    <link href="http://sharedcss.com/cdn/inspinia/css/plugins/sweetalert/sweetalert.css" rel="stylesheet">
    <script src="http://sharedcss.com/cdn/inspinia/js/plugins/sweetalert/sweetalert.min.js"></script>

    <link href="~/Content/progressbar.css" rel="stylesheet" /> 
    <script src="https://code.jquery.com/jquery-1.11.3.min.js"></script>
    <script src="~/Scripts/jquery.showLoading.min.js"></script> 
</head>
<body>
    @if (AsistenteMatricula.Portal.Security.SecurityLocal.IsAuthenticated)
    {
        <nav class="navbar navbar-expand-lg navbar-dark bg-info  fixed-top" id="navbarheader">
            @Html.ActionLink("Inicio", "Index", "Home", null, new { @class = "navbar-brand a-navbar"  }) 
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item"> 
                        @Html.ActionLink("Reporte Ranking", "RankingDocentes", "Reporte", null, new { @class = "navbar-brand a-navbar" })
                                                 
                    </li> 
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </nav> 
    }
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p class="m-t"><small>© 2019 haacc.</small></p>
            <p class="m-t"> <small>Todos los derechos reservados</small> </p>
        </footer>
    </div>
      
</body>
</html>
